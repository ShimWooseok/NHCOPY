/*
 * JS 파일이 변경되면 
 */
var publishLastUpdated = (new Date().getYear() + 1900) + '' + (new Date().getMonth() + 1) + '' + new Date().getDate() + '' + new Date().getHours();

document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/common.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/date.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/format.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/util.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/validation.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/popup.js?"+publishLastUpdated+"'></script>" ); 
document.write ( "<script type='text/javascript' charset='utf-8' src='/service/js/common/constant.js?"+publishLastUpdated+"'></script>" ); 